{
  "name": "demo",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "predev": "node scripts/kill-dev-ports.js",
    "build": "node scripts/patch-cloudflare-containers.js && bun x @sveltejs/kit sync && bun x vite build && node scripts/patch-worker.js",
    "vite": "vite",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "deploy": "alchemy deploy $([ -f ../.env ] && echo '--env-file ../.env' || echo '')",
    "destroy": "alchemy destroy --env-file ../.env",
    "dev": "alchemy dev --env-file ../.env",
    "prepare": "svelte-kit sync || echo ''",
    "preview": "vite preview",
    "gate:docs": "bun run gates/docs/llms-txt.gate.ts",
    "gate:all": "bun run gate:docs"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250805.0",
    "@lucide/svelte": "^0.563.1",
    "@sveltejs/adapter-auto": "^6.0.1",
    "@sveltejs/adapter-cloudflare": "^7.0.3",
    "@sveltejs/kit": "^2.21.2",
    "@sveltejs/vite-plugin-svelte": "^5.1.0",
    "@tailwindcss/vite": "^4.1.9",
    "alchemy": "^0.83.1",
    "clsx": "^2.1.1",
    "shiki": "^1.0.0",
    "svelte": "^5.33.14",
    "svelte-check": "^4.2.1",
    "tailwind-merge": "^3.4.0",
    "tailwind-variants": "^3.2.2",
    "tailwindcss": "^4.1.9",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3",
    "vite": "^6.3.5"
  },
  "dependencies": {
    "@cloudflare/sandbox": "^0.7.0",
    "@ai-sdk/openai": "^3.0.18",
    "@ai-sdk/openai-compatible": "^2.0.18",
    "ai": "^6.0.49",
    "marked": "^17.0.1",
    "zod": "^4.3.6"
  }
}
